n    }\r\n}\r\n\r\n// 전역설정 !global(플래그) 변수의 유효범위를 설정\r\n$color: red;\r\n.box3{\r\n    width:$w;\r\n    height:$h;\r\n    $color: #111 !global;\r\n    background:$color;\r\n}\r\n.box3_1{\r\n    width:$w;\r\n    height:$h;\r\n    background:$color\r\n}\r\n.box3_2{\r\n    $color:blue;\r\n    width:$w;\r\n    height:$h;\r\n    background:$color\r\n}\r\n\r\n// 변수 초기값 설정 !default(플래그) 할당되지 않은 변수의 초기값 설정 할당되어 있는 변수가 있다면 변수가 기존 할당 값을 사용\r\n$color: red;\r\n.box4{\r\n    width:$w;\r\n    height:$h;\r\n    $color: #111 !default;\r\n    background:$color;\r\n}\r\n.box4_1{\r\n    $color: #111 !default;\r\n    width:$w;\r\n    height:$h;\r\n    background:$color\r\n}\r\n\r\n// color 연산\r\n.box5{\r\n    width:$w;\r\n    height:$h;\r\n    $color:rgba(0,0,0,.5);\r\n    color:opacify($color, .3); // 30% 더 불투명하게 / .5 + .3\r\n    background-color:transparentize($color, .2) // 20% 더 투명하게 / .5 - .2\r\n}\r\n\r\n/*\r\n논리연산 if\r\n    ㄴand 그리고\r\n    ㄴor 또는\r\n    ㄴnot 부정(분대)\r\n*/\r\n.box6{\r\n    $w:100px;\r\n    display:block;\r\n    @if ($w > 50px and $w > 90px){\r\n        width:400px;\r\n    }\r\n    @if not ($w < 50px){ // 조건의 반대\r\n        width:400px;\r\n    }\r\n}\r\n\r\n// 가변인수의 개념\r\n.box7{\r\n    @include var(\r\n        100px,\r\n        200px,\r\n        url(\"image/01.png\") no-repeat 10px 20px,\r\n        url(\"image/02.png\") no-repeat\r\n    );\r\n}\r\n.box8{\r\n    div{\r\n        // 매개변수 순서와 개수에 맞게 전달\r\n        $font-values: italic, bold, 16px, sans-serif;\r\n        @include font($font-values...);\r\n    }\r\n    span{\r\n        // 필요한 값만 키워드 인수로 변수에 담아 전달\r\n        $font-values: (style: italic, size: 22px); // map 데이터 형태\r\n        @include font($font-values...);\r\n    }\r\n    a{\r\n        // 필요한 값만 키워드 인수로 전달\r\n        @include font((weight:900, family: monospace)...);\r\n    }\r\n}\r\n\r\n// @content\r\n.box9{\r\n    @include icon(\"image/01.png\") {\r\n        display:block;\r\n        width:$w;\r\n        height:$h;\r\n    };\r\n}\r\n\r\n// @extend\r\n.box10{\r\n    @extend .box5;\r\n    background:darkblue;    \r\n}","_mixin.scss":"\r\n// 가변하는 인수 ...\r\n@mixin var($w, $h, $bg...){\r\n    width:$w;\r\n    height:$h;\r\n    background:$bg;\r\n}\r\n\r\n@mixin font(\r\n    $style: normal,\r\n    $weight: normal,\r\n    $size: 16px,\r\n    $family: sans-serif\r\n){\r\n    font: {\r\n        style: $style;\r\n        weight: $weight;\r\n        size: $size;\r\n        family: $family;\r\n    }\r\n}\r\n\r\n// @content\r\n@mixin icon ($url){\r\n    &::after{\r\n        content:$url;\r\n        @content;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"31bafe9a8f74db0a778ae38992f37ffa","cacheData":{"env":{}}}